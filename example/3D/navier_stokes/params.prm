subsection mesh
  set mesh_path = ../../../mesh/3D/sampleA/sampleA.msh
end

subsection boundary conditions
  # --- file path for boundary consitions info ---
  set bc_data_path = inputs/sampleA/bc.h5
end

subsection finite element
  set degree_vel = 1
  set degree_pre = 1
end

subsection fluid
  set dt = 0.00005 # [s]
  set t_end = 1.92 # [s]
  set mu = 0.0035  # [Pa s]
  set rho = 1050   # [kg/m^3]
end

set output_dir = sampleA
set output_interval = 80
set verbose = false
