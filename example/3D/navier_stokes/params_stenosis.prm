subsection mesh
  set mesh_path = ../../../mesh/3D/stenosis_v1/stenosis.msh
end

subsection boundary conditions
  # ---- file path for boundary consitions info
  set bc_data_path = ../bc_time_series/output/bc_time_series.h5
end

subsection finite element
  set degree_vel = 1
