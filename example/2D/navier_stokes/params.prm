subsection mesh
  set mesh_path = ../../../mesh/2D/aneurysmD/aneurysmD.msh
end

subsection boundary conditions
  # --- file path for boundary consitions info ---
  set bc_data_path = inputs/aneurysmD/bc.h5
end

subsection finite element
  set degree_vel = 1
  set degree_pre = 1
end

subsection fluid
  set dt = 0.0002  # [s]
  set t_end = 4    # [s]
  set mu = 0.0035  # [Pa s]
  set rho = 1050   # [kg/m^3]
end

set output_dir = aneurysmD
set output_interval = 50
set verbose = false
