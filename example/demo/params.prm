subsection mesh
  set mesh_path = ../../mesh/rectangle_fine.msh
end

subsection boundary conditions
  # --- file path for boundary consitions info ---
  set bc_data_path = bc/bc.h5
end

subsection finite element
  set degree_vel = 1
  set degree_pre = 1
end

subsection fluid
  set dt = 0.01    # [s]
  set t_end = 5    # [s]
  set mu = 1       # [Pa s]
  set rho = 1000    # [kg/m^3]
end

set output_dir = demo
set output_interval = 10
set verbose = false

