subsection mesh
  set mesh_path = ../../../mesh/2D/bend_tube/bend_tube_extended_scaled.msh
end

subsection boundary conditions
  set bc_data_path = inputs/bend_tube_extended/bc.h5
end

subsection finite element
  set degree_vel = 1
  set degree_pre = 1
end

subsection fluid
  set dt = 0.001   # [s]
  set t_end = 1.2  # [s]
  set mu = 0.0035  # [Pa s]
  set rho = 1050   # [kg/m^3]
end

set output_dir = bend_tube_extended
set output_interval = 10
set verbose = false

