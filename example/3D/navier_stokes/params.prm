subsection mesh
  set mesh_path = ../../../mesh/3D/aneurysm_A/aneurysm.msh
end

subsection boundary conditions
  # ---- file path for boundary consitions info
  set bc_data_path = inputs/aneurysm_A/bc.h5
end

subsection finite element
  set degree_vel = 1
  set degree_pre = 1
end

subsection fluid
  set dt = 0.0002  # [s]
  set t_end = 1.92 # [s]
  set mu = 0.0035  # [Pa s]
  set rho = 1050   # [kg/m^3]
end

set output_dir = aneurysm_A
set output_interval = 60
set verbose = false
