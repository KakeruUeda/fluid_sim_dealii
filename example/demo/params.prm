subsection mesh
  set mesh_path = ../../mesh/rectangle_medium.msh
end

subsection boundary conditions
  # --- file path for boundary consitions ---
  set bc_data_path = bcs/bc.h5
end

subsection finite element
  set degree_vel = 1
  set degree_pre = 1
end

subsection fluid
  set dt = 0.01    # [s]
  set t_end = 30   # [s]
  set mu = 1       # [Pa s]
  set rho = 100    # [kg/m^3]
end

set output_dir = demo_cavity_re100_medium_mesh
set output_interval = 10
set verbose = false